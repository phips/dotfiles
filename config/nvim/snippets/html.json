{
  "twcard": {
    "body": [
      "<div class=\"max-w-sm rounded overflow-hidden shadow-lg hover:shadow-xl transition-shadow duration-300 ease-in-out\">",
      "  <img class=\"w-full\" src=\"${1:image.jpg}\" alt=\"Card image\">",
      "  <div class=\"px-6 py-4\">",
      "    <div class=\"font-bold text-xl mb-2\">Card Title</div>",
      "    <p class=\"text-gray-700 text-base\">",
      "    ${2:example card text}",
      "    </p>",
      "  </div>",
      "  <div class=\"px-6 pt-4 pb-2\">",
      "    <span class=\"inline-block bg-gray-200 rounded-full px-3 py-1 text-sm font-semibold text-gray-700 mr-2 mb-2\">${3:#tag1}</span>",
      "    <span class=\"inline-block bg-gray-200 rounded-full px-3 py-1 text-sm font-semibold text-gray-700 mr-2 mb-2\">${4:#tag2}</span>",
      "  </div>",
      "</div>"
    ],
    "prefix": "twcard"
  },
  "twmenu": {
