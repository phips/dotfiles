{
    "app": {
        "prefix": "app",
        "body": [
            "[[on-window-detected]]",
            "if.app-id = \"${1:}\"",
            "run = [\"layout floating\"]"
        ]
    },
    "press": {
        "prefix": "press",
        "body": [
            "[[press]]",
            "name = \"$1\"",
            "url = \"$2\"",
            "siteName = \"FreeBSD Foundation Blog\"",
            "siteUrl = \"https://freebsdfoundation.org/blog/\"",
            "date = \"$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE\"",
            "author = \"FreeBSD Foundation\"",
            "description = \"$3\"",
            ""
        ],
        "description": "FF f.org Press Entry"
    }
}

